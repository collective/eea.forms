<html xmlns="http://www.w3.org/1999/xhtml"
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  xmlns:metal="http://xml.zope.org/namespaces/metal"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  i18n:domain="eea.geotags">

  <head><title></title></head>

  <body>

  <metal:view_macro define-macro="view">
  </metal:view_macro>

  <metal:edit_macro define-macro="edit">
    <metal:use use-macro="field_macro | here/widgets/field/macros/edit">
      <metal:fill fill-slot="widget_body">
        <metal:use use-macro="here/quick_upload_widget/macros/upload" />
      </metal:fill>
    </metal:use>
  </metal:edit_macro>

  <metal:search_macro define-macro="search">
    <div metal:use-macro="here/eea.geotags/macros/edit"></div>
  </metal:search_macro>

  <metal:upload define-macro="upload">
    <div class="eea-forms-quick-upload" tal:content="structure here/aq_parent/@@quick_upload" />
  </metal:upload>

</body>
</html>
